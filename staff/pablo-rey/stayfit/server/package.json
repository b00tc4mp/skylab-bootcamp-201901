{
  "name": "stayfit",
  "version": "1.0.0",
  "description": "App to manage box, gym and personal trainer session schedules",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "ts-node-dev .",
    "reinstall": "rm -rf node_modules && npm install",
    "test": "mocha --require ts-node/register --watch-extensions ts,tsx 'src/**/*.test.{ts,tsx}' --watch",
    "test:debug": "mocha --require ts-node/register --watch-extensions ts,tsx \"src/**/*.test.{ts,tsx}\" --watch --inspect",
    "test:coverage": "nyc --reporter=html --reporter=text  nyc ts-mocha 'src/**/*.test.{ts,tsx}'"
  },
  "author": "Pablo Javier Rey Sobral",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express-session": "^1.15.12",
    "@types/graphql": "^14.2.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.3",
    "@types/sinon": "^7.0.12",
    "@types/validator": "^10.11.0",
    "apollo-server": "^2.5.0",
    "apollo-server-express": "^2.5.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-session": "^1.16.1",
    "graphql": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11",
    "reflect-metadata": "^0.1.13",
    "sinon": "^7.3.2",
    "type-graphql": "^0.17.4",
    "typegoose": "^5.6.0",
    "typescript": "^3.4.5",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "faker": "^4.1.0",
    "mocha": "^6.1.4",
    "source-map-support": "^0.5.12",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.1.1",
    "ts-node-dev": "^1.0.0-pre.39",
    "uuid": "^3.3.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.*",
      "coverage/**/*.*"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  }
}
